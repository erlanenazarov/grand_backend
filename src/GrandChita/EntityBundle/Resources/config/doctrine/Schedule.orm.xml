<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="GrandChita\EntityBundle\Repository\ScheduleRepository" name="GrandChita\EntityBundle\Entity\Schedule">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="date" type="string" column="date" length="255" nullable="true"/>
    <field name="time" type="string" column="time" length="255" nullable="true"/>
    <field name="duration" type="string" column="duration" length="255" nullable="true"/>
    <field name="price" type="string" column="price" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
